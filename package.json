    "ng-zorro-antd": "9.1.2",
    "rxjs": "6.4.0",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.6",
    "@angular/cli": "9.1.6",
    "@angular/compiler-cli": "9.1.7",
    "@angular/language-service": "9.1.7",
    "@types/express": "4.17.2",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.9.5",
    "codelyzer": "5.2.1",
    "husky": "4.2.5",
